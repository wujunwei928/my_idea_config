<templateSet group="PHP">
  <template name="vv" value="echo &quot;&lt;pre&gt;&quot;;&#10;var_dump($END$);&#10;exit;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="v" value="echo &quot;&lt;pre&gt;&quot;;&#10;var_dump($END$);" toReformat="false" toShortenFQNames="true">
